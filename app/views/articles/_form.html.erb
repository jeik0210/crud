<%= form_tag(create_article_path , method: 'post') do %>
	<%= label_tag :titulo, 'titulo'%>
	<%= text_field :article, :titulo %> <br>
	<%= label_tag :descripcion, 'descripcion'%>
	<%= text_field :article, :descripcion %> <br>
	<%= label_tag :fecha, 'fecha'%>
	<%= text_field :article, :fecha %> <br>
	<%= submit_tag 'enviar'%>
<% end %>